<h1>New Event: </h1>
<%= form_for :event do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label : eventTime%><br>
    <%= f.text_field :Starting Time %>
  </p>
  
  <p>
    <%= f.label :location %><br>
    <%= f.text_field :Location %>
  </p>
  
   <p>
    <%= f.label :max %><br>
    <%= f.text_field :number of people %>
  </p>
  
  <p>
    <%= f.label :creator %><br>
    <%= f.text_field :who are you %>
  </p>
  
  <p>
    <%= f.label :description %><br>
    <%= f.text_field :description %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>